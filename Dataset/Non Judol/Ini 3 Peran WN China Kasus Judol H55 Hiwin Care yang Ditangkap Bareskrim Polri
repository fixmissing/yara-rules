<script async>
var itms_timeout = function(){
  "use strict";
  setTimeout(function(){
    $(".iklan-top-mobile-sticky > span").removeClass('itms-run');
    $(".iklan-top-mobile-sticky > span").addClass('itms-stop');
  },5000);
};

$(window).scroll(function() {
  var sp = $(window).scrollTop() + 54;
  var bp = $('#itms-box').position().top;
  
  if(sp >= bp){
    $(".iklan-top-mobile-sticky > span").addClass('itms-run');
    itms_timeout();
  }
});
</script>

															
				

