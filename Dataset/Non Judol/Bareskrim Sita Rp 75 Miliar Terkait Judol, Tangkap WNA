<script>
window.googletag = window.googletag || { cmd: [] };

document.addEventListener("DOMContentLoaded", function () {
    console.log(" Billtop VR Optimization + Viewport Refresh Running...");

    const adSlot = document.getElementById("billboard-ad");
    let lastRefreshTime = Date.now(); // Waktu terakhir refresh
    const REFRESH_INTERVAL = 30000; // 30 detik (aturan umum refresh iklan)

    const options = {
        root: null,
        rootMargin: "0px",
        threshold: 0.75 // 75% dari elemen harus terlihat agar dihitung sebagai viewable
    };

    const observer = new IntersectionObserver((entries) => {
        entries.forEach(entry => {
            if (entry.isIntersecting) {
                console.log(" Billtop Iklan masuk viewport dan terlihat oleh user!");
                adSlot.classList.add("ad-visible");

                // Cek apakah perlu refresh (jika lebih dari 30 detik sejak refresh terakhir)
                const now = Date.now();
                if (now - lastRefreshTime > REFRESH_INTERVAL) {
                    refreshAd();
                    lastRefreshTime = now;
                }
            } else {
                console.log(" Billtop Iklan keluar dari viewport!");
            }
        });
    }, options);

    observer.observe(adSlot);

    function refreshAd() {
        console.log(" Billtop Refreshing iklan...");
        googletag.cmd.push(function() {
            googletag.pubads().refresh([googletag.pubads().getSlots().find(slot => slot.getSlotElementId() === 'div-gpt-ad-1534319180227-0')]);
        });
        console.log(" Billtop Iklan telah di-refresh!");
    }
});
</script>
