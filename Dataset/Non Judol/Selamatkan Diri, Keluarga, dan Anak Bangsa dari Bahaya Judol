<script nonce="R1cyVWtIQjYzcHU4NElrMw==" type="text/javascript">
    $(document).ready(function() {
        moment.locale(`id`);
        
        const idNews = `1136517`;
        const title = `Selamatkan Diri, Keluarga, dan Anak Bangsa dari Bahaya Judol`;
        const tags = `Selamatkan Diri, Keluarga, dan Anak Bangsa dari Bahaya Judol`;
        const idRegion = `17`;

        function destroyCarousel() {
            if ($('.recent-news-box').hasClass('slick-initialized')) {
                $('.recent-news-box').slick('unslick');
            }
        }

        function applySlider() {
            $window = $(window);
            $slick_slider = $('.recent-news-box');
            settings = {
                infinite: false,
                slidesToShow: 5,
                slidesToScroll: 1,
                prevArrow: $('.recent-prev-btn'),
                nextArrow: $('.recent-next-btn'),
                responsive: [
                    {
                        breakpoint: 1279,
                        settings: {
                            slidesToShow: 4,
                            slidesToScroll: 1,
                        }
                    },
                    {
                        breakpoint: 1039,
                        settings: {
                            slidesToShow: 3,
                            slidesToScroll: 2
                        }
                    },
                    {
                        breakpoint: 767,
                        settings: {
                            slidesToShow: 2,
                            slidesToScroll: 1
                        }
                    }
                ]
            };

            $slick_slider.slick(settings);
            $window.on('resize', function() {
            if ($window.width() < 480) {
                if ($slick_slider.hasClass('slick-initialized'))
                $slick_slider.slick('unslick');
                return
            }
            if ( ! $slick_slider.hasClass('slick-initialized'))
                return $slick_slider.slick(settings);
            });
        }

        $.ajax({
            type: 'GET',
            url: `https://www.rri.co.id/v2/ajax/get-recommendation?id_news=${idNews}&title=${title}&tags=${tags}&id_region=${idRegion}`,
            headers: {
                'x-access-ajax': `$2a$08$pSJqLdOOhMYMCo5Xcd4gF.xNcGuFq/JvTzYWLodzisRGUhzJF1vVm`
            },
            beforeSend: function() {
                $('.recent-news-box').empty();
                let html = '';
                for(let i = 1; i <= 5; i++){
                    html += `
                        <a href="#" style="margin-right:20px;">
                            <div class="news-card">
                                <div class="news-card-img bg-shimmer shimmer-cardimg"></div>
                                <div class="news-card-desc">
                                    <div class="news-card-desc-block">
                                        <div class="news-title bg-shimmer shimmer-title-half"></div>
                                        <div class="news-title bg-shimmer shimmer-title"></div>
                                        <div class="news-title bg-shimmer shimmer-title"></div>
                                    </div>
                                    <div class="news-card-desc-block">
                                        <div class="news-caption bg-shimmer shimmer-block"></div>
                                        <div class="news-caption bg-shimmer shimmer-block"></div>
                                    </div>
                                </div>
                            </div>
                        </a>
                    `;
                }

                $('.recent-news-box').html(html);
            },
            dataType: 'json',
            success: function(res) {
                if (res.status == true) {
                    let data = res.data.content.results;
                    let html = '';

                    destroyCarousel();

                    for (let i = 0; i < data.length; i++) {
                        if (i >= 0) {
                            let imgParse = data[i].image;
                            let judulReadmore = '';
                            let src = '';

                            if (imgParse.featured != '') {
                                replaceImageUrl = imgParse.featured.replace(`https://rri-assets.s3.ap-southeast-3.amazonaws.com/`, "");
                                src = `https://cdn.rri.co.id/${replaceImageUrl}`;
                            } else if (imgParse.url != '') {
                                console.log('here IMAGE')
                                replaceImageUrl = imgParse.url.replace(`https://rri-assets.s3.ap-southeast-3.amazonaws.com/`, "");
                                src = `https://cdn.rri.co.id/${replaceImageUrl}`;
                            } else {
                                src = 'https://upload.wikimedia.org/wikipedia/commons/1/14/No_Image_Available.jpg';
                            }
                            html += `
                            <a href="${data[i].url_}">
                                <div class="news-card news-card_mobile">
                                    <div class="news-card-img">
                                        <img src="${src}" alt="${data[i].judul}" class="lazyload"
                                        onerror="this.onerror=null;this.src='${imgParse.featured || imgParse.url}'">
                                    </div>
                                    <div class="news-card-desc">
                                        <div class="news-title">
                                            <div class="cat bg-red">${data[i].nama_kategori}</div>
                                            <h3>${data[i].judul}</h3>
                                        </div>
                                        <div class="news-caption">
                                            <div class="news-card-caption">
                                                <span class="fa fa-clock txt-normal color-red sqbox"></span>
                                                &nbsp;${moment(data[i].published_at).fromNow()}
                                            </div>
                                            <div class="news-card-caption">
                                                <span class="fa fa-map-marker txt-normal color-red sqbox"></span>
                                                &nbsp;${data[i].nama_daerah}
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </a>
                            `;
                        }
                    }

                    $('.recent-news-box').html(html);
                    applySlider();
                }
            }
        })
    })
</script>
