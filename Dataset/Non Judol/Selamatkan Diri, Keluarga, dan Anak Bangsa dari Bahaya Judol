    // const idDaerah = '17'
    // const slugDaerah = ''
    let __idDaerah = '17'
    let __slugDaerah = 'samarinda'

    document.addEventListener('DOMContentLoaded', function() {
        const baseUrl = 'https://www.rri.co.id';
        const currentUrl = window.location.href;
        const toggleInput = document.getElementById('toggle');
        if (currentUrl.includes(`${baseUrl}/en`)) {
            toggleInput.checked = true;
        } else {
            toggleInput.checked = false;
        }
        toggleInput.addEventListener('change', function() {
            toggleInput.disabled = true;
            if (toggleInput.checked) {
                window.location.href = `${baseUrl}/en`;
            } else {
                window.location.href = baseUrl;
            }
        });
    });

    function succCallback(res) {
        const lat = res.coords.latitude;
        const lang = res.coords.longitude;
        const url = `https://www.rri.co.id/ajax-langlat?lat=${lat}&lang=${lang}`;

        $.ajax({
            type: 'GET',
            url,
            headers: {
                'x-access-ajax': '$2a$08$pSJqLdOOhMYMCo5Xcd4gF.xNcGuFq/JvTzYWLodzisRGUhzJF1vVm'
            },
            dataType: 'json',
            success: function(response) {
                const data = response.data;
                
                if (data.id_provinsi === 31) {
                    window.location.href = `https://www.rri.co.id/pusat-pemberitaan`;
                } else {
                    window.location.href = `https://www.rri.co.id/${data.kode_provinsi}`;
                }
            }
        })
    }

    function errCallback() {
        alert(`Anda terhubung ke Pusat Pemberitaan`);
    }

    $( document ).ready(function() {
        const segment1 = `samarinda`;

        if (segment1 == '') {
            const geoOps = {
                enableHighAccuracy: true,
                timeout: 60000
            }

            navigator.geolocation.getCurrentPosition(succCallback, errCallback, geoOps);
        }

        $("#openMegaMenu").click(function(){
            $("#megamenu").slideDown();
            $('body').on('scroll', function(e) {
                e.preventDefault();
                $(this).scrollTop(0);
            });
        });
        $("#closeMegaMenu").click(function(){
            $("#megamenu").slideUp();
        });

        $('#openMegaMenuSatker').click(function() {
            $('#megamenuSatker').slideDown();
            $('body').on('scroll', function(e) {
                e.preventDefault();
                $(this).scrollTop(0);
            });
        });
        $('#closeMegaMenuSatker').click(function() {
            $('#megamenuSatker').slideUp();
        });
    });
    
</script>
