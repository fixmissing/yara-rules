<script>
  $(document).ready(function() {
    // $("#logo_utama").attr("src","images/logo-rmid-3th.png");
    // $("#logo_utama").attr("src","images/logo-rmid-rm-25-white.png");
    var hash = window.location.hash;    
    // alert(hash);    
    if(hash=='#search_button') {
      $('.nav_search').trigger('click');
    } else if(hash=='#menu_button') {
      $('#menu_button').trigger('click');
      
    }
    $("#button_live_streaming").click(function() {
      // $('html, body').animate({scrollTop : 0},500);
      $('html, body').animate({
          // scrollTop: $("#streaming_kpu").offset().top
          
          scrollTop: $("#div_streaming").offset().top - 100
      }, 2000);
      $("#youtube_live")[0].src += "?autoplay=1";          
      $("#button_live_streaming").hide();
    });
    $("#button_live_streaming").hide();
  });
</script>
