$(document).ready(function() {
  
      $('#image-gallery').lightSlider({
          gallery:true,
          item:1,

          thumbItem:5,
          pager: true,
          galleryMargin: 5,
          thumbMargin: 5,
          currentPagerPosition: 'middle',
          /* currentPagerPosition: 'middle', */
