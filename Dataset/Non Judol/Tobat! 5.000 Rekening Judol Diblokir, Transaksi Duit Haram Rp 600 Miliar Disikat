  function searchKeyword() {
    var uri = document.getElementById('search').value;
    var uri_enc = encodeURIComponent(uri);
    var uri_dec = decodeURIComponent(uri_enc);

    $.post("menu_rmid/aksi_cepat_tanggap.php?act=search_rmid", {
      search: $('#search').val()
    },
    function(data , status) {
      console.log("Data: " + encodeURIComponent(data) + "\nStatus: " + status);
      // alert();
      if(status=='success') {
        var word = data.split("---");
        // console.log(word[0] + "\n" + word[1]);
        document.location = "?search=" + encodeURIComponent(word[0]).toLowerCase().split('%20').join('+');
      }
    });
  }

  function urutBy(urut) {
    $.post("menu_rmid/aksi_cepat_tanggap.php?act=search_rmid", {
      search: $('#search_nyari_yang_ada').val(),
      urut: urut,
    },
    function(data , status) {
      if(status=='success') {
        var word = data.split("---");
        // console.log(word[0] + "\n" + word[1]);
        document.location = "?search=" + encodeURIComponent(word[0]).toLowerCase().split('%20').join('+');
      }
    });
  }

