  $(function () {
    $("#tgl_index").datepicker({ 
          format: 'dd-mm-yyyy',
          autoclose: true, 
          todayHighlight: true
    });
    $("#tgl_index").on("change",function(){
      var selected = $(this).val();
      document.location="index-berita/"+selected;
      });
  });
