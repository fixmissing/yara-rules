          loop:true,
          adaptiveHeight:true,
          onSliderLoad: function() {
              $('#image-gallery').removeClass('cS-hidden');
          }  
      });
    
    //hover images gallery
    $(".figure").hover(function(){ //alert('hover');
      if($('.figure').width()>460) {
        $('#btn_show_hide_caption_foto').show();
      }       
    }, function(){
      $('#btn_show_hide_caption_foto').hide();
    });

    $("#btn_show_hide_caption_foto").click(function(){
      if($('#btn_show_hide_caption_foto').html()=='<i class="fa fa-eye-slash"></i> Sembunyikan Caption') {
        $('#caption_foto').hide();
        $('#btn_show_hide_caption_foto').html('<i class="fa fa-eye"></i> Tampilkan Caption');
      } else {
        $('#caption_foto').show();
        $('#btn_show_hide_caption_foto').html('<i class="fa fa-eye-slash"></i> Sembunyikan Caption');
      }
    });
    // $(".figure").hover(function(){ //alert('hover');
