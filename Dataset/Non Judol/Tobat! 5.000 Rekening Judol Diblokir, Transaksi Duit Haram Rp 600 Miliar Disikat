function showInfoCache(TextInfo)
  {
  // alert(TextInfo);
  toastr.info(TextInfo, '', {timeOut: 2000});
  }
function changeResEpaper(Quality)
  {
  $.post('menu_rmid/aksi_cepat_tanggap.php?act=change_res_epaper&quality='+Quality,
  function(response)
    {
    if(response.indexOf('Berhasil') > -1)
      { location.reload(); }
    else 
      { toastr.error(response, 'Error', {timeOut: 2000}); }
    }
  );
  }
function changeTypeEpaper(Type)
  {
  $.post('menu_rmid/aksi_cepat_tanggap.php?act=change_type_epaper&type='+Type,
  function(response)
    {
    if(response.indexOf('Berhasil') > -1)
      { location.reload(); }
    else 
      { toastr.error(response, 'Error', {timeOut: 2000}); }
    }
  );
  }    
$('[data-fancybox]').fancybox({
  toolbar: false,
  smallBtn: true,
  iframe: {
    preload: true,
  },
  /* fullScreen: {
    autoStart: true
  }, */
  fullScreen: {
    autoStart: false
  },
  /* afterClose : function() {
    //alert(`Closing!`);
    //window.location.reload();
  } */
});
$('[data-fancybox="images"]').fancybox({
afterLoad : function(instance, current) {
        var pixelRatio = window.devicePixelRatio || 1;

        if ( pixelRatio > 1.5 ) {
            current.width  = current.width  / pixelRatio;
            current.height = current.height / pixelRatio;
        }
    }
});
$(this).find(".isi-berita .images_border img").css("height","5000px");
